! Xft ----------------
Xft.autohint:  0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting:   1
Xft.antialias: 1
Xft.dpi:       96
Xft.rgba:      rgb

URxvt.perl-lib: ~/.urxvt/ext/urxvt-perls
URxvt.perl-ext-common: default,matcher,...,keyboard-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
! rxvt-unicode -------
URxvt.letterSpace: -1
URxvt.font: xft:Hack:pixelsize=13:Regular
URxvt.boldFont: xft:Hack:bold:pixelsize=13:bold
URxvt.italicFont: xft:Hack:pixelsize=13:italic
URxvt.boldItalicFont: xft:Hack:pixelsize=13:bold:italic

URxvt.scrollStyle: rxvt
URxvt.scrollBar: false
URxvt.geometry: 160x45

Emacs.geometry: 202x45
Emacs.letterSpace: -1
Emacs.font: Dejavu Sans Mono:pixelsize=13:Regular
Emacs.boldFont: Dejavu Sans Mono:pixelsize=13:Bold
Emacs.italicFont: Dejavu Sans Mono:pixelsize=13:Italic
Emacs.boldItalicFont: Dejavu Sans Mono:pixelsize=13:Bold:Italic
