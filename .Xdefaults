! Xft ----------------
Xft.autohint:  0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting:   1
Xft.antialias: 1
Xft.dpi:       96
Xft.rgba:      rgb

URxvt.perl-lib: ~/.urxvt/ext/urxvt-perls
URxvt.perl-ext-common: default,matcher,...,keyboard-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
! rxvt-unicode -------
URxvt.letterSpace: -1
URxvt.font: xft:Dejavu Sans Mono:pixelsize=13:Regular
URxvt.boldFont: xft:Dejavu Sans Mono:pixelsize=13:Bold
URxvt.italicFont: xft:Dejavu Sans Mono:pixelsize=13:Italic
URxvt.boldItalicFont: xft:Dejavu Sans Mono:pixelsize=13:Bold:Italic
! solarized color theme -------
*background: #002b36
*foreground: #657b83
!!*fading: 40
*fadeColor: #002b36
*cursorColor: #93a1a1
*pointerColorBackground: #586e75
*pointerColorForeground: #93a1a1

!! black dark/light
*color0: #073642
*color8: #002b36

!! red dark/light
*color1: #dc322f
*color9: #cb4b16

!! green dark/light
*color2: #859900
*color10: #586e75

!! yellow dark/light
*color3: #b58900
*color11: #657b83

!! blue dark/light
*color4: #268bd2
*color12: #839496

!! magenta dark/light
*color5: #d33682
*color13: #6c71c4

!! cyan dark/light
*color6: #2aa198
*color14: #93a1a1

!! white dark/light
*color7: #eee8d5

URxvt.scrollStyle: rxvt
URxvt.scrollBar: false
URxvt.geometry: 160x45

Emacs.geometry: 202x45
Emacs.letterSpace: -1
Emacs.font: Dejavu Sans Mono:pixelsize=13:Regular
Emacs.boldFont: Dejavu Sans Mono:pixelsize=13:Bold
Emacs.italicFont: Dejavu Sans Mono:pixelsize=13:Italic
Emacs.boldItalicFont: Dejavu Sans Mono:pixelsize=13:Bold:Italic
